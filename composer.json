    "keywords": ["laravel","urlify","transliterate","translit","transliteration","url","encode","slug","link","iconv"],
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Manuel Garcia Olivares",
            "email": "jmgolivares@gmail.com",
            "homepage": "http://www.garciaolivares.me/",
            "role": "Main Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.2.x"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "j0an\\Urlify": "src/"
        }
    },
    "minimum-stability": "dev"
